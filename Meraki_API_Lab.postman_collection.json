		{
			"name": "A. getOrganizationNetworks",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"/*",
							"This script processes the response data from getOrganizationNetworks,",
							"and create collection variables for Branch and Campus Network IDs",
							"based on their network names.",
							"*/",
							"let networks = pm.response.json();",
							"",
							"let branchNetwork = networks.find(nw => nw.name === \"API-Branch\" || nw.name === \"Postman-Branch\" );",
							"let campusNetwork = networks.find(nw => nw.name === \"API-Campus\" || nw.name === \"Postman-Campus\");",
							"",
							"if (branchNetwork) {",
							"    pm.collectionVariables.set(\"branchnwId\", branchNetwork.id);",
							"    console.log(\"branchnwId set to:\", branchNetwork.id);",
							"} else {",
							"    console.warn(\"API-Branch network not found.\");",
							"}",
							"",
							"if (campusNetwork) {",
							"    pm.collectionVariables.set(\"campusnwId\", campusNetwork.id);",
							"    console.log(\"campusnwId set to:\", campusNetwork.id);",
							"} else {",
							"    console.warn(\"API-Campus network not found.\");",
							"}"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"/*",
							"This script checks the existence of collection variable: organizationId",
							"*/",
							"let orgId = pm.collectionVariables.get(\"organizationId\");",
							"",
							"if (!orgId || orgId.trim() === \"\") {",
							"    throw new Error(\"Missing or invalid collection variable: organizationId. Please set it before proceeding.\");",
							"}"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
